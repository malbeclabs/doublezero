      neighbor {{index .PeerVpnv4 "ld4-dz01"}} activate
      neighbor {{index .PeerVpnv4 "la2-dz01"}} activate
      neighbor {{index .PeerVpnv4 "sg1-dz01"}} activate
      neighbor {{index .PeerVpnv4 "ams-dz001"}} activate
      neighbor {{index .PeerVpnv4 "frk-dz01"}} activate
      neighbor {{index .PeerVpnv4 "pit-dzd01"}} activate
