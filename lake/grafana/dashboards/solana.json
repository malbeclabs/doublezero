                    "rawQuery": true,
                    "rawSql": "SELECT\n  time,\n  CAST(AVG(cluster_slot - last_vote_slot) AS DOUBLE) as \"Avg Vote Lag (slots)\",\n  CAST(PERCENTILE_CONT(0.5) WITHIN GROUP (ORDER BY cluster_slot - last_vote_slot) AS DOUBLE) as \"Median Vote Lag (slots)\",\n  CAST(PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY cluster_slot - last_vote_slot) AS DOUBLE) as \"P95 Vote Lag (slots)\",\n  CAST(PERCENTILE_CONT(0.99) WITHIN GROUP (ORDER BY cluster_slot - last_vote_slot) AS DOUBLE) as \"P99 Vote Lag (slots)\",\n  CAST(MAX(CASE WHEN NOT is_delinquent THEN cluster_slot - last_vote_slot ELSE NULL END) AS DOUBLE) as \"Max Vote Lag (non-delinquent)\"\nFROM solana_vote_account_activity_raw\nWHERE time >= $__timeFrom()\n  AND time <= $__timeTo()\n  AND (CASE WHEN '${vote_account:raw}' = '$__all' THEN true ELSE vote_account_pubkey IN (SELECT unnest(string_to_array('${vote_account:raw}', ','))) END)\n  AND ('${is_delinquent:raw}' = '$__all' OR is_delinquent::text IN (SELECT unnest(string_to_array('${is_delinquent:raw}', ','))))\n  AND cluster_slot >= last_vote_slot\nGROUP BY time\nORDER BY time",
                    "refId": "A"
                }
            ],
