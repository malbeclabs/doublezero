      rd 65342:1
      route-target import vpn-ipv4 65342:1
      route-target export vpn-ipv4 65342:1
      router-id {{.DeviceIP}}
      no neighbor 169.254.0.1
      neighbor 169.254.0.1 remote-as 65000
      neighbor 169.254.0.1 local-as 65342 no-prepend replace-as
