                    "rawSql": "WITH period_stats AS (\n  SELECT\n    MAX(cluster_slot) - MIN(cluster_slot) as slot_range,\n    COUNT(DISTINCT time) as sample_count\n  FROM solana_vote_account_activity_raw\n  WHERE time >= $__timeFrom()\n    AND time <= $__timeTo()\n),\nvalidator_credits AS (\n  SELECT\n    va.vote_account_pubkey,\n    va.node_identity_pubkey,\n    va.activated_stake_sol,\n    va.commission,\n    va.is_delinquent,\n    CAST(AVG(va.cluster_slot - va.last_vote_slot) AS DOUBLE) as avg_vote_lag,\n    CAST(MAX(va.cluster_slot - va.last_vote_slot) AS DOUBLE) as max_vote_lag,\n    CAST(AVG(va.cluster_slot - va.root_slot) AS DOUBLE) as avg_root_lag,\n    CAST(MAX(va.cluster_slot - va.root_slot) AS DOUBLE) as max_root_lag,\n    CAST(SUM(va.credits_delta) AS DOUBLE) as total_credits_accrued,\n    CAST(MAX(va.credits_epoch_credits) AS DOUBLE) as current_epoch_credits,\n    COUNT(va.credits_delta) as non_null_delta_count,\n    MIN(va.time) as first_seen,\n    MAX(va.time) as last_seen\n  FROM solana_vote_account_activity_raw va\n  WHERE va.time >= $__timeFrom()\n    AND va.time <= $__timeTo()\n    AND ('${vote_account:raw}' = '$__all' OR va.vote_account_pubkey IN (SELECT unnest(string_to_array('${vote_account:raw}', ','))))\n    AND ('${is_delinquent:raw}' = '$__all' OR va.is_delinquent = ('${is_delinquent:raw}' = 'true'))\n  GROUP BY va.vote_account_pubkey, va.node_identity_pubkey, va.activated_stake_sol, va.commission, va.is_delinquent\n),\nmax_credits AS (\n  SELECT MAX(total_credits_accrued) as max_credits_earned\n  FROM validator_credits\n)\nSELECT\n  vc.vote_account_pubkey as \"Vote Account\",\n  vc.node_identity_pubkey as \"Node Identity\",\n  CAST(COALESCE(vc.activated_stake_sol, 0) AS DOUBLE) as \"Activated Stake (SOL)\",\n  CAST(COALESCE(vc.commission, 0) AS DOUBLE) as \"Commission %\",\n  CASE\n    WHEN mc.max_credits_earned > 0 AND vc.non_null_delta_count > 0 THEN CAST((vc.total_credits_accrued / mc.max_credits_earned * 100.0) AS DOUBLE)\n    ELSE NULL\n  END as \"Earned Credits %\",\n  vc.is_delinquent as \"Delinquent\",\n  vc.avg_vote_lag as \"Avg Vote Lag (slots)\",\n  vc.max_vote_lag as \"Max Vote Lag (slots)\",\n  vc.avg_root_lag as \"Avg Root Lag (slots)\",\n  vc.max_root_lag as \"Max Root Lag (slots)\",\n  vc.total_credits_accrued as \"Total Credits Accrued\",\n  vc.current_epoch_credits as \"Current Epoch Credits\"\nFROM validator_credits vc\nCROSS JOIN max_credits mc\nCROSS JOIN period_stats ps\nWHERE vc.non_null_delta_count > 0\nORDER BY \"Activated Stake (SOL)\" DESC\nLIMIT 100",
                    "refId": "A"
                }
            ],
            "title": "Vote Account Summary",
            "transformations": [],
            "type": "table"
        }
